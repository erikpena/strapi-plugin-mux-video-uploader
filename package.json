{
  "name": "strapi-plugin-mux-video-uploader",
  "version": "2.8.4",
  "description": "This plugin allows you to upload your content to Mux and use it with Strapi.",
  "main": "./dist",
  "strapi": {
    "name": "mux-video-uploader",
    "displayName": "Mux Video Uploader",
    "icon": "plug",
    "description": "This plugin allows you to upload your content to Mux and use it with Strapi.",
    "kind": "plugin"
  },
  "author": {
    "name": "Erik Peña",
    "email": "erikpena@users.noreply.github.com",
    "url": "https://github.com/erikpena"
  },
  "maintainers": [
    {
      "name": "Erik Peña",
      "email": "erikpena@users.noreply.github.com",
      "url": "https://github.com/erikpena"
    }
  ],
  "scripts": {
    "build": "yarn clean && tsc && yarn copy-files",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles README.md yarn.lock strapi-admin.js strapi-server.js dist/",
    "dev": "tsc --watch",
    "preprepare": "husky install"
  },
  "dependencies": {
    "@mux/mux-node": "^8.2.0",
    "@mux/mux-player-react": "^2.3.3",
    "@mux/upchunk": "^3.3.2",
    "@mux/videojs-kit": "^0.12.0",
    "@strapi/icons": "^2.0.0-rc.11",
    "@strapi/utils": "^4.20.5",
    "axios": "^1.6.7",
    "copy-to-clipboard": "^3.3.3",
    "formik": "^2.4.5",
    "iso-639-1": "^3.1.2",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.20.2",
    "react-router-dom": "^5.3.4",
    "styled-components": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@strapi/design-system": "2.0.0-beta.6",
    "@strapi/strapi": "^5.0.0",
    "@types/koa": "^2.15.0",
    "@types/lodash": "^4.17.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.59",
    "@types/react-dom": "^18.2.19",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.34",
    "copyfiles": "^2.4.1",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "pretty-quick": "^4.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.2"
  },
  "peerDependencies": {
    "@strapi/strapi": "5.0.0"
  },
  "exports": {
    "./strapi-admin": {
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/src/index.js",
      "require": "./dist/admin/src/index.js",
      "default": "./dist/admin/src/index.js"
    },
    "./strapi-server": {
      "source": "./server/index.ts",
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "strapi-server.js"
  ],
  "license": "MIT"
}
